	<!-- Page Content -->
<div class="profile">
	<form action="/example" method="POST">
  		<h1><span>Create your profile.</span></h1>

  		<div class="form-box">
			<label for="fileupload"> Upload your Resume.</label>
			<br><input type="file" name="fileupload" value="fileupload" id="fileupload">
			<button type="button" class="submit">Upload</button>
			<br>
			<br>
			<label for="first"><b>First Name</b></label>
			<input type="text" placeholder="Enter First Name" name="firstname" required>

			<label for="last"><b>Last Name</b></label>
			<input type="text" placeholder="Enter Last Name" name="lastname" required>

			<label for="email"><b>Email</b></label>
			<input type="text" placeholder="Enter Email" name="email" required>

			<label for="location"><b>Location</b></label>
			<input type="text" placeholder="Tempe, AZ" name="location" required>

			<label for="skills"><b>Skills</b></label>
			<input type="text" placeholder="Java, HTML, JavaScript..." name="skills" required>

		</div>

		<div class="clearfix">
			<button type="button" class="cancelbtn">Cancel</button>
			<button type="submit" class="signupbtn" formaction="memberIndex.html">Create Profile</button>
		</div>
	</form>
</div>

